{
  "name": "partybillcalculatorplus",
  "version": "1.0.0",
  "type": "module",
  "description": "PartyBillCalculatorPlus（PBC+ 記賬）是一個由於 DAE 課程而製作的在線網頁，能幫不同的用戶，實現多設備網頁端記賬的功能。主要用於聚會、聚餐之後的計算賬單的計算，讓不同的人可以依照規則計算自己的消費。",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "node --loader ts-node/esm server/server.ts",
    "dev:watch": "nodemon",
    "dev:all": "concurrently \"npm run dev\" \"npm run ocr:dev\"",
    "dev:all:watch": "concurrently \"npm run dev:watch\" \"npm run ocr:dev\"",
    "build": "tsc",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "lint": "eslint \"**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "install-concurrently": "npm install concurrently nodemon --no-save",
    "db:ui": "erd-ui erd.txt",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:dev": "run-s db:migrate db:plan db:update",
    "db:migrate": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex migrate:latest",
    "db:seed": "node --loader ts-node/esm seed.ts",
    "db:plan": "auto-migrate dev.sqlite3 < erd.txt && node --loader ts-node/esm scripts/fix-migration.ts",
    "db:rename": "auto-migrate --rename dev.sqlite3 < erd.txt && node --loader ts-node/esm scripts/fix-migration.ts",
    "db:update": "run-s db:migrate db:gen-proxy",
    "db:gen-proxy": "erd-to-proxy < erd.txt > server\\proxy.ts && node --loader ts-node/esm scripts/fix-proxy.ts",
    "db:fix-proxy": "node --loader ts-node/esm scripts/fix-proxy.ts",
    "db:fix-migration": "node --loader ts-node/esm scripts/fix-migration.ts",
    "ocr:setup": "cd ocr-service && echo 請參考 source venv310/Scripts/activate && python main.py",
    "ocr:dev": "cd ocr-service && source venv310/Scripts/activate && python main.py",
    "ocr:test": "curl http://localhost:8000/health",
    "llm:test": "node --loader ts-node/esm scripts/test-prompt.ts",
    "llm:test:v1": "node --loader ts-node/esm scripts/test-prompt.ts v1",
    "llm:test:v2": "node --loader ts-node/esm scripts/test-prompt.ts v2",
    "llm:test:v3": "node --loader ts-node/esm scripts/test-prompt.ts v3",
    "reset-usage": "node --loader ts-node/esm scripts/reset-user-usage.ts",
    "train:level1": "node --loader ts-node/esm scripts/train/train-level1.ts",
    "train:level2": "node --loader ts-node/esm scripts/train/train-level2.ts",
    "train:level3": "node --loader ts-node/esm scripts/train/train-level3.ts",
    "train:python:level1": "cd food-recognition-service && python train/train_level1.py",
    "train:python:level2": "cd food-recognition-service && python train/train_level2.py",
    "train:python:level3": "cd food-recognition-service && python train/train_level3.py",
    "train:python:convert": "cd food-recognition-service && python convert/convert_to_tfjs.py",
    "test:python:training": "cd food-recognition-service && python test_training.py",
    "test:python:model": "cd food-recognition-service && python test_nodejs_load.py",
    "test:model:load": "node --loader ts-node/esm scripts/test-model-load.ts",
    "data:download": "node --loader ts-node/esm scripts/data-collection/download-datasets.ts",
    "build:tensorflow": "bash scripts/build-tensorflow.sh",
    "build:tensorflow:cmd": "scripts\\build-tensorflow.cmd",
    "build:tensorflow:ps1": "powershell -ExecutionPolicy Bypass -File scripts\\build-tensorflow.ps1",
    "test:tensorflow": "node --loader ts-node/esm scripts/test-tensorflow-build.ts",
    "check:env": "powershell -ExecutionPolicy Bypass -File scripts\\check-env-vars.ps1",
    "find:kits": "powershell -ExecutionPolicy Bypass -File scripts\\find-windows-kits.ps1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucaXJX/PartyBillCalculatorPlus.git"
  },
  "keywords": [
    "bill-calculator",
    "expense-splitting",
    "party-bill",
    "group-expenses",
    "typescript",
    "express",
    "nodejs"
  ],
  "author": "LucaXJX",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucaXJX/PartyBillCalculatorPlus/issues"
  },
  "homepage": "https://github.com/LucaXJX/PartyBillCalculatorPlus#readme",
  "dependencies": {
    "@mistralai/mistralai": "^1.10.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/form-data": "^2.5.2",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "best-effort-json-parser": "^1.2.1",
    "better-sqlite3": "^11.10.0",
    "better-sqlite3-proxy": "^2.11.3",
    "better-sqlite3-schema": "^3.1.9",
    "cast.ts": "^1.19.0",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "form-data": "^4.0.5",
    "formidable": "^3.5.4",
    "fs-extra": "^11.3.2",
    "html2canvas": "^1.4.1",
    "image-dataset": "^1.12.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "p-queue": "^9.0.1",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "populate-env": "^2.4.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/formidable": "^3.4.6",
    "@types/fs-extra": "^11.0.4",
    "@types/integer": "^4.0.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.25",
    "@types/pg": "^8.15.6",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "express": "^5.1.0",
    "livereload": "^0.9.3",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "quick-erd": "^4.29.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp",
      "bcrypt",
      "@tensorflow/tfjs-node"
    ]
  }
}
